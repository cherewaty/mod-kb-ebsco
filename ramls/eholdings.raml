                      }
                    ],
                    "jsonapi": {
                      "version": "1.0"
                    }
                  }
        404:
          description: Not Found
          body:
            application/vnd.api+json:
              example: |
                {
                  "errors": [
                    {
                      "title": {"errors":[{"code":1001,"subCode":0,"message":"Vendor not found"}]}
                    }
                  ],
                  "jsonapi": {
                    "version": "1.0"
                  }
                }
        422:
          description: Unprocessable Entity
          body:
            application/vnd.api+json:
              example: |
                {
                  "errors": [

                    "allowKbToAddTitles": false
                  },
                  "relationships": {
                    "resources": {
                      "meta": {
                        "included": false
                      }
                    },
                    "vendor": {
                      "meta": {
                        "included": false
                      }
                    },
                    "provider": {
                      "meta": {
                        "included": false
                      }
                    }
                  }
                },
                "jsonapi": {
                  "version": "1.0"
                }
              }
      400:
        description: Bad Request
        body:
          application/vnd.api+json:
              example: |
                {
                  "errors": [
                    {
                      "title": {"errors":[{"code":1009,"subCode":0,"message":"Custom Package with the provided name already exists"}]}
                    }
                  ],
                  "jsonapi": {
                    "version": "1.0"
                  }
                }
